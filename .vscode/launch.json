  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal"
    },
    {
        "name": "Debug - Custom Pigeon File",
        "type": "python",
        "request": "launch",
        "program": "${workspaceFolder}/cmdline.py",
        "args": [
            "${file}"
        ],
        "console": "integratedTerminal"
    },
    {
        "name": "Debug - Custom Pigeon Graph File",
        "type": "python",
        "request": "launch",
        "program": "${workspaceFolder}/cmdline.py",
        "args": [
            "${file}",
            "--graph"
        ],
        "console": "integratedTerminal"
    }

]
}
